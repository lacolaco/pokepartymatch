{
  "name": "pokepartymatch",
  "version": "1.7.0",
  "scripts": {
    "ng": "ng",
    "start": "run-p 'start:*'",
    "start:build": "ng build --watch",
    "start:firebase": "firebase emulators:start",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "deploy:staging": "firebase deploy --only hosting:staging",
    "deploy:production": "firebase deploy --only hosting:production,firestore",
    "postinstall": "cp -R node_modules/pokesprite-images/pokemon-gen8 src/assets/",
    "format": "prettier --write src/app",
    "tools:pokemon-data": "ts-node --project tsconfig.tools.json tools/pokemon-data.ts",
    "release": "standard-version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "11.0.5",
    "@angular/compiler": "11.0.5",
    "@angular/core": "11.0.5",
    "@angular/fire": "6.1.4",
    "@angular/forms": "11.0.5",
    "@angular/platform-browser": "11.0.5",
    "@angular/platform-browser-dynamic": "11.0.5",
    "@angular/router": "11.0.5",
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-regular-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@lacolaco/reactive-store": "4.0.0",
    "@ngrx/component": "10.1.2",
    "firebase": "8.2.1",
    "ng-zorro-antd": "10.2.2",
    "pokesprite-images": "2.5.0",
    "rxjs": "6.6.3",
    "tslib": "2.1.0",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1000.0",
    "@angular-devkit/build-angular": "0.1100.5",
    "@angular/cli": "11.0.5",
    "@angular/compiler-cli": "11.0.5",
    "@types/jasmine": "3.6.2",
    "@types/node": "12.19.12",
    "codelyzer": "6.0.1",
    "firebase-tools": "9.1.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.6.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "standard-version": "9.0.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.0.5"
