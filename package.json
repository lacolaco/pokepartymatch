{
  "name": "pokemonbuild",
  "version": "1.1.1",
  "scripts": {
    "ng": "ng",
    "start": "run-p 'start:*'",
    "start:build": "ng build --watch",
    "start:firebase": "firebase emulators:start",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:staging": "firebase deploy --only hosting:staging",
    "deploy:production": "firebase deploy --only hosting:production,firestore",
    "postinstall": "cp -R node_modules/pokesprite-images/pokemon-gen8 src/assets/",
    "format": "prettier --write src/app",
    "tools:pokemon-data": "ts-node --project tsconfig.tools.json tools/pokemon-data.ts",
    "release": "standard-version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.9",
    "@angular/common": "10.0.9",
    "@angular/compiler": "10.0.9",
    "@angular/core": "10.0.9",
    "@angular/fire": "6.0.2",
    "@angular/forms": "10.0.9",
    "@angular/platform-browser": "10.0.9",
    "@angular/platform-browser-dynamic": "10.0.9",
    "@angular/router": "10.0.9",
    "@ant-design/icons-angular": "9.0.1",
    "@lacolaco/reactive-store": "4.0.0",
    "@ngrx/component": "10.0.0",
    "firebase": "7.18.0",
    "ng-zorro-antd": "9.2.2",
    "pokesprite-images": "2.2.4",
    "rxjs": "6.5.5",
    "tslib": "2.0.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1000.0",
    "@angular-devkit/build-angular": "0.1000.6",
    "@angular/cli": "10.0.6",
    "@angular/compiler-cli": "10.0.9",
    "@types/jasmine": "3.5.12",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.54",
    "codelyzer": "6.0.0",
    "firebase-tools": "8.7.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.1.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "protractor": "7.0.0",
    "standard-version": "9.0.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  }
}
